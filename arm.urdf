<robot name="robot_arm">
 	<link name="base_link">
    <!--
    <intertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
  
      <mass
        value="0" />>
  
      <intertia
        ixx="0"
        ixy="0"
        ixz="0"
        iyy="0"
        iyz="0"
        izz="0" />
    </intertial>
    -->

		<visual>
		  <origin xyz="0 0 0.0225" rpy="0 0 0"/>
			<geometry>
				<box size=".20 .20 .05"/>
			</geometry>
		</visual>	
	</link>

  <link name="TSL">
		<visual>
          	<geometry>
                <cylinder length=".0175" radius=".06"/>
			</geometry>
		</visual>	
	</link>
    
  <joint name="turntable" type="revolute">
    <parent link="base_link"/>
    <child link="TSL"/>
    <origin xyz="0 .0 .05" rpy="0 0 0" />
    <axis xyz="0 0 -1"/>
    <limit lower="-1.5" upper="1.5" /> 
  </joint>
    
  <link name="upper_arm">
		<visual>
          <origin xyz="0 0 0.26" rpy="0 0 0" />
          	<geometry>
            	<box size=".11 .09 .51"/>
			</geometry>
		</visual>	
	</link>
    <joint name="shoulder" type="revolute">
      <parent link="TSL"/>
      <child link="upper_arm"/>
      <origin xyz="0 0.06 0.075" rpy="0 0 0" />
      <axis xyz="1 0 0"/>
      <limit lower="-1.5" upper="1.5" />
    </joint>
    
    <link name="EFL">
		<visual>
          <origin xyz="0.0 -0.06 0" rpy="0 0 0" />
          	<geometry>
            	<box size=".085 .16 .115"/>
			</geometry>
		</visual>	
	</link>

    <joint name="elbow" type="revolute">
      <parent link="upper_arm"/>
      <child link="EFL"/>
      <origin xyz="0 0.0 0.51" rpy="0 0 0" />
      <axis xyz="1 0 0"/>
        <limit lower="-3.14" upper="1" />
    </joint>
    
    <link name="forearm">
		<visual>
          <origin xyz="0 0 0.25" rpy="0 0 0" />
          	<geometry>
            	<box size="0.07 0.07 0.38" />
			</geometry>
		</visual>	
    </link>

    <joint name="forearm_rotation" type="revolute">
      <parent link="EFL"/>
      <child link="forearm"/>
      <origin xyz="0 -0.1 0" rpy="0 0 0" />
      <axis xyz="0 0 -1"/>  
	  <limit lower="-3.14" upper="3.14" /> 
    </joint>
    
    <link name="hand">
		<visual>
          <origin xyz="0 0 0.21" rpy="0 0 0" />
          	<geometry>
            	<cylinder length="0.42" radius="0.04"/>
			</geometry>
		</visual>	
	</link>

  <joint name="wrist" type="revolute">
      <parent link="forearm"/>
      <child link="hand"/>
      <origin xyz="0 0 0.48" rpy="0 0 0" />
      <axis xyz="1 0 0"/>
      <limit lower="-2" upper="2" />       
    </joint>
    
    <link name="end_effector">
		<visual>
          <origin xyz="0 0 0.38" rpy="0 0 0" />
          	<geometry>
            	<box size="0.02 0.02 0.02" />
			</geometry>
		</visual>	
	</link>

    <joint name="hand_rotation" type="revolute">
      <parent link="hand"/>
      <child link="end_effector"/>
      <origin xyz="0 0 0.048" rpy="0 0 0" />
      <axis xyz="0 0 1"/>
	  <limit lower="-6" upper="6" /> 
    </joint>
    
</robot>